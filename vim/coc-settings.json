{
	"coc.preferences.formatOnSaveFiletypes": ["javascript", "json"],
	"coc.preferences.currentFunctionSymbolAutoUpdate": true,
	"suggest.echodocSupport": true,
	"suggest.minTriggerInputLength": 3,
	"codeLens.enable": true,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.errorSign": "•",
	"diagnostic.warningSign": "•",
	"diagnostic.infoSign": "•",
	"rust.clippy_preference": "on",
	"list.insertMappings": {
		"<C-v>": "action:vsplit",
		"<C-s>": "action:split"
	},
	"list.normalMappings": {
		"d": "action:delete"
	},
	"list.source.files.args": ["--hidden", "--files"],
	"list.source.files.excludePatterns": ["**/.git/**"],
	"languageserver": {
		"flow": {
			"command": "flow",
			"args": ["lsp"],
			"filetypes": ["javascript"],
			"initializationOptions": {},
			"requireRootPattern": true,
			"settings": {},
			"rootPatterns": [".flowconfig"]
		},
		"golang": {
			"command": "gopls",
			"args": ["serve"],
			"filetypes": ["go"],
			"initializationOptions": {}
		},
		"terraform": {
			"command": "terraform-lsp",
			"args": [],
			"filetypes": ["terraform"]
		}
	},
	"diagnostic-languageserver.filetypes": {
		"markdown": ["write-good", "markdownlint"],
		"sh": "shellcheck",
		"php": "phpcs"
	},
	"suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79be",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	}
}
